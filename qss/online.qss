QTreeView::item:hover, QTreeView::branch:hover {
	background-color: rgb(239,239,239);
}
QTreeView::item:selected, QTreeView::branch:selected {
    background-color: rgb(192,218,255);
}

QWidget#login {
    min-width: 350px;
    min-height: 400px;
    max-width: 450px;
    max-height: 600px;
    background-color: rgb(111, 119, 124);
    /* image:url(); */
}

QLabel#label_welcome {
    color: rgb(0,204,255);
    font : 20px "Microsoft YaHei";
}
QLabel#label_onLine {
    color: white;
    font : 30px "Microsoft YaHei";
}
QLineEdit#lineEdit_passwd,
QLineEdit#lineEdit_user,
QLineEdit#le_account,
QLineEdit#le_email {    
    min-width: 250px;
    min-height: 40px;
    max-width: 250px;
    max-height: 40px;
    padding-left: 40px;
    padding-right: 10px;
    font-size: 20px;
    background-color: #7F8488;
    border: 1px solid white;
    border-radius: 20px;
}
QLineEdit#le_account,
QLineEdit#le_email {    
    padding-left: 10px;
}
QLineEdit#lineEdit_passwd:focus,
QLineEdit#lineEdit_user:focus,
QLineEdit#le_account:focus,
QLineEdit#le_email:focus,
QLineEdit#lineEdit_passwd:hover,
QLineEdit#lineEdit_user:hover,
QLineEdit#le_account:hover,
QLineEdit#le_email:hover {
    background-color: rgb(145, 178, 206);
}
QLineEdit#lineEdit_passwd QPushButton#more { 
    /* border: 0; */
    image:url(:/login/Resources/images/16.png);
}
QLineEdit#lineEdit_user QPushButton#more {
    /* border: 0; */
    image:url(:/login/Resources/images/1.png);
}

QPushButton#btn_login{
    height: 30px;
    width: 200px;
    font-size: 16px ;
	color:rgb(0,105,203);
	background-color:rgba(255,255,255);
	border:1px solid rgba(24,112,211);
	border-radius :15px;
}
QPushButton#btn_login:hover{
	color:rgb(255,255,255);
	background-color:rgba(19,143,229);
	border:1px solid rgb(19,143,229);
}
QPushButton#btn_login:pressed{
	color:rgb(255,255,255);
	background-color:rgb(24,112,211);
	border:1px solid rgb(24,112,211);
}

QCheckBox {
    color: white;
    font-size: 15px ;
}
QPushButton#btn_accunt_next,
QPushButton#btn_email_prev,
QPushButton#btn_email_next,
QPushButton#btn_return,
QPushButton#btn_forgot_password{
    border: 0;
    height: 30px;
    width: 250px;
    font-size: 16px ;
    color:rgb(255,255,255);
}

QPushButton#btn_accunt_next:hover,
QPushButton#btn_email_prev:hover,
QPushButton#btn_email_next:hover,
QPushButton#btn_return:hover,
QPushButton#btn_forgot_password:hover{
	color:rgb(19,143,229);
}

QPushButton#btn_accunt_next:pressed,
QPushButton#btn_email_prev:pressed,
QPushButton#btn_email_next:pressed,
QPushButton#btn_return:pressed,
QPushButton#btn_forgot_password:pressed{
	color:rgb(5, 80, 121);
}

