/* login */
QWidget {
    font-family: "Microsoft YaHei";
}
QTableWidget{
    border:1px solid #BDC9FF;
    font:bold 9pt "Microsoft YaHei";
    background-color: rgb(255,255,255);
}
QTableWidget QHeaderView::section {
    min-height:25px;
    min-width: 73px;
    /* text-align: left; */
    /* padding-left:3px;
    padding-right:3px;	 */
    background-color: rgb(255,255,255);
    /* background-color: qlineargradient(x1:0, y1:0, x2:0, y2:1,stop:0 #fefefe,stop:1 #F0F0F0); */
}
QTableWidget QHeaderView::section:first{
    border-bottom: 1px solid #BDC9FF;
    border-top: 0;
    border-left:0;
}
QTableWidget QHeaderView::section:last{
    border-bottom: 1px solid #BDC9FF;
    border-top: 0;
    border-right:0;
    border-left:1px solid #BDC9FF;	
}
QTableWidget::item:hover {
	color: #000000;
	background: #EFEFEF; 
}
QTableWidget::item:selected {
	color: #000000;
	background: #C0DAFF; 
}

QTreeView {
    padding: 5px;
    outline: 0px;
    border: none;
    show-decoration-selected: 1;
    background-color: white;
}
QTreeView::item {
    height: 25px;
    border: none;
    background-color: transparent;  
	text-align:center;
}
/*QTreeView::branch {
     background-color: transparent;   
}*/
